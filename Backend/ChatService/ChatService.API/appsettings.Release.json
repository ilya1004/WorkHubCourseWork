{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoDbSettings": {
    "ConnectionString": "mongodb://root:mongopass@mongodb:27017",
    "DatabaseName": "ChatServiceDb"
  },
  "AzuriteSettings": {
    "ConnectionString": "UseDevelopmentStorage=true;DevelopmentStorageProxyUri=http://azurite:10000;",
    "FilesContainerName": "chat-files"
  },
  "JwtSettings": {
    "SecretKey": "my_secret_key_my_secret_key_my_secret_key_my_secret_key_my_secret_key",
    "Issuer": "IdentityService",
    "Audience": "WorkHubServices"
  },
  "Logstash": {
    "Url": "http://logstash:5000"
  },
  "Elasticsearch": {
    "Url": "http://elasticsearch:9200"
  }
}